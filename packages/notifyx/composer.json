{
    "name": "notifyx/notifyx",
    "description": "A powerful Laravel package to provide a frontend + backend in-app notification system for modern Laravel applications",
    "keywords": [
        "laravel",
        "notifications",
        "livewire",
        "real-time",
        "in-app",
        "notifyx"
    ],
    "license": "MIT",
    "type": "library",
    "authors": [
        {
            "name": "Saurabh Punia",
            "email": "chaudharysaurabhpunia@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.0",
        "orchestra/testbench": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Notifyx\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Notifyx\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Notifyx\\NotifyxServiceProvider"
            ],
            "aliases": {
                "Notifyx": "Notifyx\\Facades\\Notifyx"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
